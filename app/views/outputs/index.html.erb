<% groupName = current_user.memberships.first.group.name %>

<div class="panel-heading">Output for: <%=groupName%></div>

<div class="panel-body">
<table class="table table-striped table-hover">
  <tbody>
      <tr>
            <td><h3>Population</h3>
          <%= link_to image_tag("/assets/images/"+groupName+"/small/population.jpg"),"#population", class: "fancybox" %>
            <div style="display:none;">
              <div id="population">
                <%=image_tag("/assets/images/"+groupName+"/population.jpg")%>
              </div>
            </div>
          </td>

         <td><h3><%=link_to "Percentage Sick","#curPercentage",class: "fancybox"%></h3>
           <div style="display:none;">
             <div id="curPercentage">
               <%=image_tag("/assets/images/"+groupName+"/percentage2.jpg")%>
             </div>
           </div>
          <%= link_to image_tag("/assets/images/"+groupName+"/small/percentage.gif"),"#percentage", class: "fancybox" %>
            <div style="display:none;">
              <div id="percentage">
                <%=image_tag("/assets/images/"+groupName+"/percentage.gif")%>
              </div>
            </div>
         </td>
      </tr>
      <tr>
    <td><h3>Actual Sick</h3>
    <%= link_to image_tag("/assets/images/"+groupName+"/small/sick.gif"),"#sick", class: "fancybox" %>
      <div style="display:none;">
        <div id="sick">
          <%=image_tag("/assets/images/"+groupName+"/sick.gif")%>
        </div>
      </div>
    </td>
    <td><h3>Sick & Immune</h3>
     <%= link_to image_tag("/assets/images/"+groupName+"/small/siChart.jpg"),"#sickChart", class: "fancybox" %>
       <div style="display:none;">
         <div id="sickChart">
           <%=image_tag("/assets/images/"+groupName+"/siChart.jpg")%>
         </div>
       </div>
    </td>
   </tr>
   <tr>
     <td><h3>Population Pie</h3>
     <%= link_to image_tag("/assets/images/"+groupName+"/small/PopPieChart.jpg"),"#PPC", class: "fancybox" %>
       <div style="display:none;">
         <div id="PPC">
           <%=image_tag("/assets/images/"+groupName+"/PopPieChart.jpg")%>
         </div>
       </div>
     </td>
     <td><h3>Sick Pie</h3>
      <%= link_to image_tag("/assets/images/"+groupName+"/small/SicPieChart.jpg"),"#SPC", class: "fancybox" %>
        <div style="display:none;">
          <div id="SPC">
            <%=image_tag("/assets/images/"+groupName+"/SicPieChart.jpg")%>
          </div>
        </div>
     </td>
    </tr>
  </tbody>
</table>

</div>

<%= stylesheet_link_tag "http://ajax.googleapis.com/ajax/libs/jqueryui/1.7.2/themes/redmond/jquery-ui.css", "application" %>
<%= javascript_include_tag "http://ajax.googleapis.com/ajax/libs/jquery/1.4.2/jquery.min.js", "http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.1/jquery-ui.min.js", "application" %>


